<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">
  <display-name>MaterialJSF</display-name>
  <welcome-file-list>
  <!-- Definição do JavaServer Faces -->
  <servlet>
  	<servlet-name>Faces Servlet</servlet-name>
  	<servlet-class>javax.faces.web.app.FacesServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>  
  <servlet-mapping>
  	<servlet-name>Faces Servlet</servlet-name>
  	<url-pattern>*.jsf</url-pattern>
  </servlet-mapping>
  </welcome-file-list>
</web-app>